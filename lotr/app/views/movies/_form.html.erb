<%= render partial: "shared/error_list", locals: {object: @movie} %>

<%=form_for @movie do |f|%>
<%=f.label :name%>
<%=f.text_field :name%>
<%=f.label :release_year%>
<%=f.number_field :release_year%>
<%=f.label :description%>
<%=f.text_field :description%><br>
<%=f.label :add_character%>
<%=f.collection_check_boxes :character_ids, Character.all, :id, :name%>
<%=f.submit%> 
<%end%>